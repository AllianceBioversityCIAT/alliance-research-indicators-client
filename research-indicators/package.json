{
  "name": "research-indicators",
  "version": "0.0.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-dev": "ng build --configuration development",
    "watch": "ng build --watch --configuration development",
    "test": "jest --config jest.config.ts",
    "test:watch": "jest --watch --config jest.config.ts",
    "test:coverage": "jest --coverage --config jest.config.ts",
    "lint": "ng lint",
    "s-lint": "stylelint '**/*.scss'",
    "docker:build": "docker build -t research_indicatiors_client .",
    "docker:run": "docker run --env-file ./.env -dti --name research_indicatiors_client_dev -p 4200:4200 research_indicatiors_client",
    "docker:stop": "docker stop research_indicatiors_client_dev && docker rm research_indicatiors_client_dev",
    "docker:logs": "docker logs research_indicatiors_client_dev",
    "docker:exec": "docker exec -it research_indicatiors_client_dev /bin/bash",
    "compose:up:dev": "docker-compose up dev -d",
    "compose:up:prod": "docker-compose up prod -d",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.6",
    "@angular/cdk": "^19.1.4",
    "@angular/common": "^19.1.6",
    "@angular/compiler": "^19.1.6",
    "@angular/core": "^19.1.6",
    "@angular/forms": "^19.1.6",
    "@angular/platform-browser": "^19.1.6",
    "@angular/platform-browser-dynamic": "^19.1.6",
    "@angular/router": "^19.1.6",
    "@angular/service-worker": "19.1.6",
    "@hotjar/browser": "^1.0.9",
    "@microsoft/clarity": "^1.0.0",
    "@primeng/themes": "^19.0.6",
    "chart.js": "^4.4.7",
    "chartjs-plugin-datalabels": "^2.2.0",
    "driver.js": "^1.3.6",
    "echarts": "^6.0.0",
    "exceljs": "^4.4.0",
    "ibdevkit": "^0.2.1",
    "ngx-socket-io": "^4.7.0",
    "pdfjs-dist": "^3.11.174",
    "primeicons": "^7.0.0",
    "primeng": "^19.0.6",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^19.1.7",
    "@angular/cli": "^19.1.7",
    "@angular/compiler-cli": "^19.1.6",
    "@types/jest": "^29.5.12",
    "angular-eslint": "^19.1.0",
    "eslint": "^9.3.0",
    "jest-preset-angular": "^14.5.1",
    "prettier": "^3.3.3",
    "stylelint": "^16.6.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.24.1"
  }
}
